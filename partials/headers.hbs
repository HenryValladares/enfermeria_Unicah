<header>
    <!-- Barra de navegación principal -->
    <nav class="navbar navbar-expand-lg">
        <div class="container-fluid">
            <!-- Logotipo de la Facultad de Enfermería -->
            <img src="/images/logoEn.jpg" alt="Facultad de Enfermeria">

            <!-- Botón de menú para pantallas pequeñas -->
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon outline-warning"></span>
            </button>

            <!-- Contenido del menú colapsable -->
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <!-- Lista de navegación generada dinámicamente -->
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    {{#each navegacion}}
                    <li class="nav-item">
                        <!-- Enlace de navegación -->
                        <a class="nav-link" href="{{url}}">{{title}}</a>
                    </li>
                    {{/each}}
                </ul>

                <!-- Formulario de búsqueda -->
                <form class="d-flex" role="search">
                    <!-- Campo de entrada para la búsqueda -->
                    <input class="form-control me-2" type="search" placeholder="Buscar" aria-label="Search">
                    <!-- Botón para enviar la búsqueda -->
                    <button class="btn btn-outline-warning" type="submit">Buscar</button>
                </form>
            </div>
        </div>
    </nav>
</header>